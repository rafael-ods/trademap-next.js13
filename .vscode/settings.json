{
  "files.associations": {
    "*.css": "tailwindcss"
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": true,
    "source.fixAll.eslint": true
  },
  "eslint.run": "onSave",
  "cSpell.language": "en,pt_BR",
  "cSpell.words": [
    "commitlint",
    "divvisible",
    "faqs",
    "heineken",
    "squadfy",
    "tailwindcss",
    "trademap",
    "Trademap",
    "xeerpa",
    "xlarge",
    "xsmall",
    "xxgiant",
    "xxlarge"
  ],
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true
}
